{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django runserver",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "runserver",
        "0.0.0.0:8000"
      ],
      "env": {
        "DJANGO_SETTINGS_MODULE": "{{ name }}.settings"
      },
      "python": "/usr/local/bin/python",
      "django": true,
      "justMyCode": true
    }
  ]
}
